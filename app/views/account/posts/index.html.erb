<div class="col-md-12">
  <h2 class="text-center">My Posts</h2>

  <table class="table">
    <thead>
      <tr>
        <th>#</th>
        <th>Content</th>
        <th></th>
        <th></th>
        <th>Last update</th>
        <th colspan="2"></th>
      </tr>
    </thead>

    <tbody>
      <% @posts.each do |post| %>
        <tr>
          <td>#</td>
          <td><%= post.content%> </td>
          <td><%=  %></td>
          <td><%=  %></td>
          <td><%= post.updated_at %> </td>
          <td>
            <%= link_to("edit", edit_group_post_path(post.group, post), class: "btn btn-default")%>
            <%= link_to("dump", account_post_path(post), method: :delete, data: { confirm: "Suer?" }, class: "btn btn-default")%>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
